{
    "manifest_version": 3,
    "name": "Habitomo - Habit Tracker & Pomodoro Timer",
    "description": "A realistic weekly habit tracker combined with a Pomodoro timer.",
    "version": "1.0",
    "author": "Abhishek Jha",
    "homepage_url": "https://www.abhishekjha.com",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "permissions": [
      "storage",
      "tabs"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    }
  }
  